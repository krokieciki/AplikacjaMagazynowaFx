<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="198.0" prefWidth="391.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ConfirmationController">
   <children>
      <AnchorPane prefHeight="232.0" prefWidth="369.0" style="-fx-background-color: #D8D8D8;">
         <children>
            <Label layoutX="46.0" layoutY="30.0" prefHeight="86.0" prefWidth="299.0" text="Are you sure you want to delete current account?" textFill="#000000bd" wrapText="true">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <AnchorPane layoutY="146.0" prefHeight="86.0" prefWidth="391.0" style="-fx-background-color: #bc4848;">
               <children>
                  <Button fx:id="cancelButton" layoutX="69.0" layoutY="33.0" mnemonicParsing="false" onAction="#cancelButtonAction" prefHeight="28.0" prefWidth="84.0" text="Cancel" textFill="#544444">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="deleteButton" layoutX="231.0" layoutY="27.0" mnemonicParsing="false" onAction="#deleteButtonAction" prefHeight="41.0" prefWidth="106.0" text="Delete" textAlignment="CENTER" textFill="#bc4848">
                     <font>
                        <Font size="19.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
